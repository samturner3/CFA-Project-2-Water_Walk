<!DOCTYPE html>
<html>
  <head>
    <title>WaterWalk</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'http://maps.google.com/maps/api/js?key=AIzaSyBbzHf-PPjzFjiNoRTSjbQ6VTt6zXZMZMA&libraries=places' %>

    <%#= javascript_include_tag 'https://code.jquery.com/jquery-1.10.2.min.js' %>
    <%= javascript_include_tag('locationpicker.jquery.min.js') %>
  </head>

  <body>
    <%= render 'layouts/nav' %>


      <% if current_user %>
        <div class="well well-sm">
          Admin: <strong><%= current_user.admin? %></strong><br>
          <%= link_to 'Toggle admin rights', admin_path(current_user), method: :patch, class: 'btn btn-info' %>
        </div>
      <% end %>


      <div class="container">
        <%= render partial: 'layouts/notice', flash: flash %>
        <%= yield %>
      </div>
  </body>
</html>
